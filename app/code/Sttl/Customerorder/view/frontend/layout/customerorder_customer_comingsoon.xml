<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" label="Customer My Account (All Pages)" design_abstraction="custom">
<update handle="customer_account"/>
    <head>
        <remove src="Magestorm_ReCaptcha::css/default.css"/>
        <remove src="css/print.css"/>
        <remove src="knockoutjs/knockout.js"/>
    </head>
<body>
    <referenceContainer name="content">
        <referenceBlock name="page.main.title" remove="true" />
        <block class="Magento\Framework\View\Element\Template" name="comingsoon" cacheable="false" template="Sttl_Customerorder::comingsoon.phtml" />
    </referenceContainer>
</body>
</page>