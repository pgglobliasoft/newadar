{
    "name": "ebizcharge/ebizcharge",
    "description": "Allows the processing of credit card payments via the Ebizcharge Gateway. You must generate a sourcekey in your merchant console.",
    "require": {
        "php": "~5.6.0|~7.0.0",
        "magento/module-config": "100.0.*",
        "magento/module-sales": "100.0.*",
        "magento/module-payment": "100.0.*",
        "magento/module-directory": "100.0.*",
        "magento/module-checkout": "100.0.*",
        "magento/framework": "100.0.*"
    },
    "type": "magento2-module",
    "version": "1.0.0",
    "license": [
        "proprietary"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Ebizcharge\\Ebizcharge\\": ""
        }
    },
    "extra": {
        "map": [
            [
                "*",
                "Ebizcharge/Ebizcharge"
            ]
        ]
    }
}